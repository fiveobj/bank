<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/item_money_my_deposit_fname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="定期存款（元）"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="8dp"
        android:textColor="#ff484e54"
        android:textSize="14sp"
        />

    <TextView
        android:id="@+id/item_money_my_deposit_fmoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3333.00"
        android:layout_marginTop="38dp"
        android:layout_marginLeft="8dp"
        android:textColor="#ff484e54"
        android:textSize="14sp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginRight="12dp"
        android:layout_alignParentRight="true"
        android:text="总收益"
        android:textColor="#ff484e54"
        android:textSize="14sp"
        />

    <TextView
        android:id="@+id/item_money_my_deposit_fprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+333.00"
        android:layout_marginTop="38dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="12dp"
        android:textColor="#ffda412f"
        android:textSize="14sp"
        />
    <ImageView
        android:id="@+id/item_money_my_deposit_fline"
        android:layout_marginTop="70dp"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#EBEBEB"/>
</RelativeLayout>